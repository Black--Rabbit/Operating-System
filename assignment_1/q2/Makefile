exec: q2_input.o baggage.o luggage_security.o weight.o boarding_pass.o immigration.o passport.o visa.o main.c #security.o male_security.o main.c
	gcc -o exec  -g main.c q2_input.o baggage.o luggage_security.o weight.o boarding_pass.o immigration.o passport.o visa.o #security.o male_security.o
	
