	gcc -g -c boarding_pass.c
	
immigration.o: immigration.c
	gcc -g -c immigration.c
	
passport.o: passport.c
	gcc -g -c passport.c
	
visa.o: visa.c
	gcc -g -c visa.c

security.o: security.c
	gcc -g -c security.c

